/* components/detail-rank-area/detail-rank-area.wxss */
.rank-name {
  width: 100%;
  height: 50px;
  font-size: 24px;
  font-weight: 700;
  line-height: 50px;
  padding-left: 10px;
  background-color: green;
}

.rank-area {
  width: 100%;
  padding: 10px 10px;
}

.rank-area-item {
  display: flex;
  align-items: center;
  height: 60px;
}

.rank-area-item .index {
  width: 10%;
  font-size: 16px;
  text-align: center;
  line-height: 50px;
}

.rank-area-item .music-info {
  font-size: 16px;
}

.rank-area-item .music-alMame {
  margin-top: 5px;
  height: 18px;
  line-height: 18px;
  font-size: 14px;
  color: #817d7d;
}

.rank-area-item .music-alMame::before {
  content: "";
  display: inline-block;
  vertical-align: middle;
  width: 28px;
  height: 16px;
  background: url(data:image/png;base64,);
  background-size: cover;
}
