/* components/area-header/area-header.wxss */
.area-header {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 40px;
  background-color: yellow;
}

.title {
  font-size: 24px;
  font-weight: 700;
  line-height: 40px;
}

.more {
  display: flex;
}

.more-text {
  line-height: 40px;
  color: rgb(107, 106, 105);
}

.more-pic {
  width: 30px;
  height: 30px;
  color: red;
  margin-top: 5px;
}
